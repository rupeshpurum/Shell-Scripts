<tl1-profile id="fujitsu-flashwave-9500" version="c2cb0d4de946"> 
  <!-- ==================================================================== -->  
  <!-- Enumeration mappings -->  
  <!-- ==================================================================== -->  
  <map name="AlarmAction"> 
    <entry tl1="CL" model="CLEAR"/>  
    <entry tl1="SC" model="RAISE"/>  
    <entry tl1="TC" model="RAISE"/>  
    <default model="RAISE"/> 
  </map>  
  <map name="ServiceAffecting"> 
    <entry tl1="SA" model="true"/>  
    <default model="false"/> 
  </map>  
  <map name="PerceivedSeverity"> 
    <entry tl1="CR" model="CRITICAL"/>  
    <entry tl1="MJ" model="MAJOR"/>  
    <entry tl1="MN" model="MINOR"/>  
    <entry tl1="NA" model="WARNING"/>  
    <entry tl1="SC" model="WARNING"/>  
    <entry tl1="CL" model="WARNING"/>  
    <entry tl1="TC" model="INFO"/>  
    <entry tl1="NR" model="NOT_REPORTED"/> 
  </map>  
  <map name="Transitory"> 
    <entry tl1="TC" model="true"/>  
    <default model="false"/> 
  </map>  
  <map name="Direction"> 
    <entry tl1="RCV" model="RECEIVE"/>  
    <entry tl1="TRMT" model="TRANSMIT"/>  
    <default model="NA"/> 
  </map>  
  <map name="ProbableCause"> 
    <default model="INDETERMINATE"/> 
  </map>  
  <map name="Location"> 
    <entry tl1="FEND" model="FAR_END"/>  
    <entry tl1="NEND" model="NEAR_END"/>  
    <default model="NA"/> 
  </map>  
  <map name="TimePeriod"> 
    <entry tl1="1-DAY" model="DAY_1"/>  
    <entry tl1="15-MIN" model="MIN_15"/>  
    <default model="OTHER"/> 
  </map>  
  <!-- ==================================================================== -->  
  <!-- Alarm definitions -->  
  <!-- ==================================================================== -->  
  <event object="centina.sa.model.fault.EquipmentAlarm"> 
    <verb>REPT ALM</verb>  
    <verb-block> 
      <verb-position index="2"> 
        <map-property name="aidType" default=""/> 
      </verb-position> 
    </verb-block>  
    <position-block> 
      <position index="0"> 
        <property name="sourceId"/>  
        <map-property name="aidVal" default=""/> 
      </position> 
    </position-block>  
    <position-block> 
      <position index="0"> 
        <property name="perceivedSeverity" map="PerceivedSeverity"/>  
        <property name="action" map="AlarmAction"/> 
      </position>  
      <position index="1"> 
        <property name="probableCause" map="ProbableCause"/>  
        <property name="specificProblem"/> 
      </position>  
      <position index="2" property="serviceAffecting" map="ServiceAffecting"/>  
      <position index="5" property="location" map="Location"/>  
      <position index="6" property="direction" map="Direction"/> 
    </position-block>  
    <position-block> 
      <position index="0" property="additionalInformation"/> 
    </position-block>  
    <map> 
      <property name="sourceName" value="$aidType$:$aidVal$"/> 
    </map>  
    <example><![CDATA["FP-1-1:CR,OLOS,SA,08-20,12-14-06,NEND,RCV:\"Optical Loss Of Signal\""]]></example> 
  </event>  
  <!-- ==================================================================== -->  
