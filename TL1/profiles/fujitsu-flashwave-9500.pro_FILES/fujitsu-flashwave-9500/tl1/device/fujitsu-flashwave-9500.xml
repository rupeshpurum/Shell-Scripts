<?xml version="1.0" encoding="UTF-8"?>

<tl1-profile id="fujitsu-flashwave-9500" version="8eef3f9788ae"> 
  <!-- ==================================================================== -->  
  <!-- Meta -->  
  <!-- ==================================================================== -->  
  <meta> 
    <vendor name="Fujitsu"/>  
    <type name="ONU"/>  
    <productPattern name="FLASHWAVE_9500|FLASHWAVE_9500_SDS"/>  
    <versionPattern name=".*"/>  
    <capabilities> 
      <complexCTAG>true</complexCTAG>  
      <maxATAG>999999</maxATAG>  
      <rowsPerResponse>20</rowsPerResponse>  
      <retrieveType>true</retrieveType>  
      <discovery>true</discovery>  
      <synchronization>true</synchronization>  
      <inventory>true</inventory> 
    </capabilities>  
    <requires> 
      <keepAlive>true</keepAlive>  
      <inhibitPm>false</inhibitPm> 
    </requires> 
  </meta>  
  <!-- ==================================================================== -->  
  <!-- command-set definitions -->  
  <!-- ==================================================================== -->  
  <command-set name="login"> 
    <command> 
      <parameter name="commandCode" value="ACT-USER"/>  
      <parameter name="tid" property="name"/>  
      <parameter name="aid" property="uid"/>  
      <position-block> 
        <position index="0" property="pid"/> 
      </position-block> 
    </command>  
    <command> 
      <parameter name="commandCode" value="RTRV-SYS"/>  
      <parameter name="tid" property="name"/> 
    </command> 
  </command-set>  
  <!-- ==================================================================== -->  
  <command-set name="logout"> 
    <command> 
      <parameter name="commandCode" value="CANC-USER"/>  
      <parameter name="tid" property="name"/>  
      <parameter name="aid" property="uid"/> 
    </command> 
  </command-set>  
  <!-- ==================================================================== -->  
  <command-set name="keep-alive"> 
    <command> 
      <parameter name="commandCode" value="RTRV-HDR"/>  
      <parameter name="tid" property="name"/> 
    </command> 
  </command-set>  
  <!-- ==================================================================== -->  
  <command-set name="inhibit-pm"> 
    <command> 
      <parameter name="commandCode" value="INH-PMREPT-ALL"/>  
      <parameter name="tid" property="name"/> 
    </command> 
  </command-set>  
  <command-set name="allow-pm"> 
    <command> 
      <parameter name="commandCode" value="ALW-PMREPT-ALL"/>  
      <parameter name="tid" property="name"/> 
    </command> 
  </command-set>  
  <!-- ==================================================================== -->  
  <command-set name="retrieve-type-and-version"> 
    <command> 
      <parameter name="commandCode" value="RTRV-NETYPE"/>  
      <parameter name="tid" property="name"/>  
      <response object="centina.sa.model.topology.NetworkElement"> 
        <position-block> 
          <position index="0" property="vendorName"/>  
          <position index="1" property="productName"/>  
          <position index="3" property="productVersion"/> 
        </position-block>  
        <explicit> 
          <property name="deviceType" value="ONU"/> 
        </explicit>  
        <example><![CDATA["Fujitsu,FLASHWAVE_9500_SDS,PS-DSLTM-P2MP,3.00-501V130310"]]></example> 
      </response> 
    </command> 
  </command-set> 
</tl1-profile>
